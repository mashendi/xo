<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.paint.LinearGradient?>
<?import javafx.scene.paint.Stop?>
<?import javafx.scene.text.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" id="AnchorPane" prefHeight="400.0" prefWidth="600.0"
            styleClass="bodybg3" stylesheets="@../Style/GameStyle.css"
            xmlns="http://javafx.com/javafx/8.0.171" fx:controller="screen.FreeOnlinePlayersController">
    <children>
        <BorderPane prefHeight="500.0" prefWidth="700.0">
            <center>
                <Pane prefHeight="200.0" prefWidth="200.0" styleClass="bodybg3" stylesheets="@../Style/GameStyle.css"
                      BorderPane.alignment="CENTER">
                    <children>
                        <ListView fx:id="listViewOnline" layoutX="43.0" layoutY="183.0"
                                  onMouseClicked="#handleMouseClickAction" prefHeight="286.0" prefWidth="284.0"
                                  stylesheets="@../Style/OnlineListViewColors.css"/>
                        <Label layoutX="236.0" layoutY="28.0" prefHeight="44.0" prefWidth="319.0"
                               text="Invite a player">
                            <font>
                                <Font name="Cambria Math" size="36.0"/>
                            </font>
                            <textFill>
                                <LinearGradient endX="1.0" endY="1.0">
                                    <stops>
                                        <Stop color="#866d6d"/>
                                        <Stop color="WHITE" offset="1.0"/>
                                    </stops>
                                </LinearGradient>
                            </textFill>
                        </Label>
                        <Button layoutX="4.0" layoutY="-1.0" mnemonicParsing="false" onAction="#back_btn"
                                prefHeight="63.0" prefWidth="61.0">
                            <graphic>
                                <ImageView fitHeight="37.0" fitWidth="37.0" pickOnBounds="true" preserveRatio="true">
                                    <image>
                                        <Image url="@../Style/previous.png"/>
                                    </image>
                                </ImageView>
                            </graphic>
                        </Button>
                        <ListView fx:id="listViewPlaying" layoutX="359.0" layoutY="180.0"
                                  onMouseClicked="#handleMouseClickAction" prefHeight="289.0" prefWidth="284.0"
                                  stylesheets="@../Style/PlayingListViewColors.css"/>
                        <ProgressIndicator fx:id="waitingIndicator" layoutX="286.0" layoutY="265.0" prefHeight="98.0"
                                           prefWidth="116.0" progress="0.0"/>
                        <Text fill="#290149" layoutX="134.0" layoutY="183.0" smooth="false" stroke="#328d3f"
                              strokeType="OUTSIDE" strokeWidth="0.0" text="Online" wrappingWidth="101.13671875">
                            <font>
                                <Font name="28 Days Later" size="31.0"/>
                            </font>
                        </Text>
                        <Text fill="#280147" layoutX="476.0" layoutY="174.0" smooth="false" stroke="#290149"
                              strokeType="OUTSIDE" strokeWidth="0.0" text="Playing" wrappingWidth="88.13671875">
                            <font>
                                <Font name="28 Days Later" size="19.0"/>
                            </font>
                        </Text>
                        <ImageView fitHeight="75.0" fitWidth="86.0" layoutX="464.0" layoutY="79.0" pickOnBounds="true"
                                   preserveRatio="true">
                            <image>
                                <Image url="@../Style/player-versus-player%20(3).png"/>
                            </image>
                        </ImageView>
                        <ImageView fitHeight="87.0" fitWidth="343.0" layoutX="133.0" layoutY="74.0" pickOnBounds="true"
                                   preserveRatio="true">
                            <image>
                                <Image url="@../Style/cloud-computing.png"/>
                            </image>
                        </ImageView>
                    </children>
                </Pane>
            </center>
        </BorderPane>
    </children>
</AnchorPane>
